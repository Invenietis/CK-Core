{
  "version": "0.0.0",

  "testRunner": "xunit",

  "dependencies": {
    "xunit.core": "2.2.0-beta5-build3474",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "FluentAssertions": "4.18.0",
    "CK.ActivityMonitor": { "target": "project" },
    "CK.ActivityMonitor.StandardSender": { "target": "project" }
  },

  "buildOptions": {
    "copyToOutput": {
      "include": [ "xunit.runner.json" ]
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      }
    },
    "net451": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.1.0"
      }
    }
  }
}
